package office.utility;

public class Person {
	private String name;
	private Date bdate;
	
	public Person() {
		//super();
		//name and bdate should not remain null
		name="";
		bdate=new Date();
	}

	public Person(String name, int dd,int mm,int yy) {
		super();
		this.name = name;
		this.bdate = new Date(dd,mm,yy);
	}

	public Person(String name, Date bdate) {
		super();
		this.name = name;
		this.bdate = bdate;
	}
	
	
	
	
	
	

}
