		List<String> words = new ArrayList<>();
		words.add("welcome");
		words.add("hi");
		words.add("object");
		words.add("html");
		words.add("jquery");
		words.add("welcome");
		words.add("bootstrap");
		words.add(null);
		words.add(null);
				
		System.out.println("No of elements : "+words.size());
		
		System.out.println(words); //toString()
		
		words.add(3,"javascript");
		
		System.out.println(words); //toString()
		
		words.remove(1);
		
		System.out.println(words); //toString()
		
		words.remove("welcome");
		
		System.out.println(words); //toString()
		
		words.set(0, "json");
		
		System.out.println(words); //toString()
	}

}
